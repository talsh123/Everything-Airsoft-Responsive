  "bugs": {
    "url": "https://github.com/talsh123/Everything-Airsoft-Responsive/issues"
  },
  "homepage": "https://github.com/talsh123/Everything-Airsoft-Responsive#readme",
  "dependencies": {
    "@sendgrid/mail": "^7.2.6",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.23.0",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.0",
    "mongoose": "^5.12.2",
    "react-scripts": "^4.0.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
